{
    "name": "fundevogel/pcbis2pdf",
    "type": "library",
    "description": "pcbis2pdf helper for use with DTP software",
    "keywords": ["pcbis", "knv", "csv", "dtp"],
    "homepage": "https://github.com/Fundevogel",
    "license": "GPL v3",
    "scripts": {
        "cs-dry": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix"
    },
    "authors": [
        {
            "name": "Martin Folkers",
            "homepage": "https://twobrain.io",
            "role": "Maintainer"
        }
    ],
    "require": {
        "doctrine/cache": "^1.8",
        "getkirby/toolkit": "^2.5",
        "scriptotek/google-books": "^0.2.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "PCBIS2PDF\\": "lib/"
        }
    }
}
